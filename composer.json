{
    "name": "consatan/phpctags",
    "description": "An enhanced php ctags index generator. Forked from vim-php/phpctags",
    "keywords": ["ctags"],
    "type": "library",
    "require": {
        "php": ">=5.6",
        "nikic/php-parser": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.6"
    },
    "autoload": {
        "classmap": ["PHPCtags.class.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\PHPCTags\\": "tests/"
        }
    },
    "bin": ["bin/phpctags"]
}
